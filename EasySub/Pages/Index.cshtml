@page
@model IndexModel
@{
    ViewData["Title"] = "Accueil - Subeasy";
}

<h1>Bienvenue sur Subeasy</h1>
<p>Découvrez nos abonnements à prix réduits.</p>

<ul>
    @foreach (var brand in Model.Brands)
    {
        <li>
            <a href="/Subscription?brandId=@brand.Id">@brand.Name</a>
        </li>
    }
</ul>
